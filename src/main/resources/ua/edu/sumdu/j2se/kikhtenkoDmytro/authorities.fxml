<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<ScrollPane fx:id="parent"
            xmlns:fx="http://javafx.com/fxml/1"
            xmlns="http://javafx.com/javafx/11"
            fitToWidth="true"
            fitToHeight="true"
            stylesheets="@css/common.css"
            fx:controller="ua.edu.sumdu.j2se.kikhtenkoDmytro.controller.AuthoritiesController"
            styleClass="bg-diluted">
    <GridPane fx:id="authoritiesContainer"
              styleClass="bg-diluted">
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
        </padding>
        <columnConstraints>
            <ColumnConstraints percentWidth="90" halignment="LEFT"/>
            <ColumnConstraints percentWidth="10" halignment="RIGHT"/>
        </columnConstraints>
    </GridPane>
</ScrollPane>
